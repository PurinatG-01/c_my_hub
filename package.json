{
  "name": "c-my-hub-monorepo",
  "version": "1.0.0",
  "description": "C My Hub - Health Tracking App Monorepo",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev:frontend": "cd frontend && flutter run",
    "dev:backend": "cd backend && npm run dev",
    "build:frontend": "cd frontend && flutter build",
    "build:backend": "cd backend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && flutter pub get"
  },
  "keywords": [
    "health",
    "tracking",
    "flutter",
    "supabase",
    "monorepo"
  ],
  "author": "",
  "license": "MIT"
}

